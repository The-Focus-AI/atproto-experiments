{
  "name": "atprot-examples",
  "version": "1.0.0",
  "description": "Practical TypeScript examples for building with AT Protocol and Bluesky - job queues, static sites, file sync, firehose, and more",
  "type": "module",
  "scripts": {
    "setup": "tsx scripts/setup.ts",
    "create-account": "tsx account/create-account.ts",
    "directory-sync": "tsx directory-sync/sync.ts",
    "web-links": "tsx directory-sync/generate-web-links.ts",
    "pds-sync": "tsx pds-sync/sync.ts",
    "pds-server": "cd pds-server && npx tsx server.ts",
    "firehose-blobs": "tsx firehose-blobs/firehose-blobs.ts",
    "firehose-filter": "tsx firehose-filter/firehose-filter.ts",
    "jetstream-filter": "tsx jetstream-filter/jetstream-filter.ts",
    "markdown-sync": "tsx markdown-sync/markdown-sync.ts",
    "job-poster": "tsx job-queue/poster.ts",
    "job-listener": "tsx job-queue/listener.ts",
    "private-groups": "tsx private-groups/example-usage.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "build": "tsc",
    "dev": "tsx watch src/index.ts"
  },
  "keywords": [
    "atproto",
    "bluesky",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/ui": "^1.0.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@atproto/api": "^0.12.0",
    "@atproto/common": "^0.4.12",
    "@atproto/identity": "^0.4.9",
    "@atproto/pds": "^0.4.192",
    "@atproto/repo": "^0.4.0",
    "@atproto/sync": "^0.1.36",
    "@atproto/xrpc": "^0.5.0",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.3.1",
    "multiformats": "^13.4.1",
    "ws": "^8.18.3"
  }
}
